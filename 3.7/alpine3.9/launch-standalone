DOCKER_IP=`ifconfig | grep "inet " | grep -Fv 127.0.0.1 | awk '{print $2}'`
docker run --rm \
-e "PUBLIC_KEY=public-key" \
-e "PRIVATE_KEY=private-key" \
-e "WIDGET_URL_PREFIX=/ic/starter/" \
-e "DEV_MODE=true" \
-e "BASE_URL=does-not-matter" \
-e "PROTO=https://" \
--name=python-starter \
-p 5001:5001 \
-p 5000:5000 \
python-starter:prod-1.0
